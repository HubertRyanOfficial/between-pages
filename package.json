  "homepage": "https://github.com/HubertRyanOfficial/between-pages#readme",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-native": "^0.70.7",
    "prettier": "^2.8.0",
    "react": "^18.1.0",
    "react-native": "^0.70.6",
